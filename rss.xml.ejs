<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>

<title><%= data.title %></title>
<link><%= data.link %></link>
<description>Daniel Rashevsky's personal website.</description>
<atom:link href="<%= data.link %>/rss.xml" rel="self" type="application/rss+xml" />

<% for (const name of Object.keys(posts).sort((p1, p2) => p1.date - p2.date)) { const {date, title, body} = posts[name]; -%>
<item>
     <title><%= title %></title>
     <link><%= data.link %>/posts/<%= name %>.html</link>
     <guid><%= data.link %>/posts/<%= name %>.html</guid>
     <pubdate><%= date.toUTCString() %></pubdate>
     <description><![CDATA[<%- body.trim() %>]]></description>
</item>
<% } -%>

</channel>
</rss>
